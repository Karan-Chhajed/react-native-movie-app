{
  "expo": {
    "name": "Movie Pal",
    "slug": "movie-pal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/cinema.png",
    "scheme": "movie",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#000000",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.deusmaximus.moviepal",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/cinema.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.deusmaximus.moviepal"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/cinema.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/cinema.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f7059067-15ae-4bb9-9793-bc89743cc961"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/f7059067-15ae-4bb9-9793-bc89743cc961"
    }
  }
}
